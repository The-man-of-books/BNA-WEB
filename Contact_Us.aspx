<script>
    function FOLDDOWN(clickedElement) {
        const parent = clickedElement.parentElement;
        const isOpen = parent.getAttribute('data-foldown') === '1';

        // Close all accordions
        const allContainers = document.querySelectorAll('[data-foldown]');
        allContainers.forEach(container => {
            container.setAttribute('data-foldown', '0');
            container.style.maxHeight = '44px';
        });

        // If the clicked one was previously closed, open it
        if (!isOpen) {
            parent.setAttribute('data-foldown', '1');
            parent.style.maxHeight = '4000px';
        }
    }
</script>






